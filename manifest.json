{
  "name": "Randial",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Random dial.",
   "content_scripts": [
    {
      "matches": ["http://*/*"],
      "js": ["jquery.min.js"]
    }
  ],
  "background": {
    "scripts": ["background.js", "jquery.min.js"]
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "tabs", "history",
    "http://random.yahoo.com/bin/ryl",
    "http://*/"
  ]
}
